{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/processed/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }